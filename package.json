{
    "name": "lantube",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "install": "node ./tools/install.js",
        "startNode": "tsc && gulp && concurrently \"npm run node\" ",
        "start": "concurrently -r \"npm run tsc:w\" \"npm run node\" ",
        "node": "nodemon -q ./app.js",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "jobs": "tsc && node jobs/scheduler.js",
        "lint": "tslint --project ."
    },
    "dependencies": {
        "body-parser": "^1.13.3",
        "castnow": "^0.4.17",
        "chromecast-js": "^0.1.7",
        "colors": "^1.1.2",
        "cookie-parser": "^1.3.3",
        "cors": "^2.8.4",
        "dotenv": "^2.0.0",
        "ejs": "^2.3.1",
        "express": "^4.16.3",
        "helmet": "^3.12.0",
        "http-status-codes": "^1.3.0",
        "ip": "^1.1.5",
        "mongoose": "^5.0.11",
        "mongoose-auto-increment": "^5.0.1",
        "morgan": "^1.6.1",
        "node-notifier": "^4.6.0",
        "pump": "^1.0.1",
        "redis": "^2.8.0",
        "request": "^2.74.0",
        "serve-favicon": "^2.3.0",
        "shelljs": "^0.7.3"
    },
    "devDependencies": {
        "@types/express": "^4.11.1",
        "@types/node": "^9.6.0",
